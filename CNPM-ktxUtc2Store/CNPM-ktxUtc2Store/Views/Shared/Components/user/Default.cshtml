@model applicationUser
<li class="login-item">
    <img src="/images/@Model.profilePicture" style="width:50px;height:50px;">
</li>
<li class="login-item">
 
    <a href="/User/wantToSaler/@Model.Id">Tôi muốn trở thành người bán hàng</a>
    <a class="nav-link text-dark" asp-area="Identity" asp-page="/Account/Manage/Index" title="Manage">
        <span id="username">@Model.fullname</span>
    </a>
</li>
